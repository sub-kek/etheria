From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: sub-kek <subbkek@gmail.com>
Date: Sun, 8 Dec 2024 14:38:38 +0300
Subject: [PATCH] Fix javadoc generation Pufferfish issue


diff --git a/build.gradle.kts b/build.gradle.kts
index aae422978887a0f2cf531b2dc862705148569676..23d0592bdb0e1424ef5ae03bffff92495565bbfe 100644
--- a/build.gradle.kts
+++ b/build.gradle.kts
@@ -169,6 +169,7 @@ tasks.jar {
 
 tasks.withType<Javadoc> {
     val options = options as StandardJavadocDocletOptions
+    options.addStringOption("-add-modules", "jdk.incubator.vector") // Etheria - fix javadoc generation Pufferfish issue
     options.overview = "src/main/javadoc/overview.html"
     options.use()
     options.isDocFilesSubDirs = true
